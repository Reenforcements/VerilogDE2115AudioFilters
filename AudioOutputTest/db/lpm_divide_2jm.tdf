FUNCTION sign_div_unsign_qlh (denominator[5..0], numerator[31..0])
RETURNS ( quotient[31..0], remainder[5..0]);

--synthesis_resources = lut 230 
SUBDESIGN lpm_divide_2jm
( 
	denom[5..0]	:	input;
	numer[31..0]	:	input;
	quotient[31..0]	:	output;
	remain[5..0]	:	output;
) 
VARIABLE 
	divider : sign_div_unsign_qlh;
	numer_tmp[31..0]	: WIRE;

BEGIN 
	divider.denominator[] = denom[];
	divider.numerator[] = numer_tmp[];
	numer_tmp[] = numer[];
	quotient[] = divider.quotient[];
	remain[] = divider.remainder[];
END;
